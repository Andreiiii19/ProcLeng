void instruccion() :
{
	Attributes att = new Attributes();	
}
{
		inst_leer(att)
	|   inst_saltar_linea(att)
	|	inst_escribir(att)
	|   inst_escribir_linea(att)
	|   inst_invocacion_o_asignacion(att)
	|   inst_if(att)
	|   inst_while(att)
	|   inst_return(att)
	|   inst_null(att)
}

